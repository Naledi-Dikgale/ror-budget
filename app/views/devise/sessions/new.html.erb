<div class="login-form">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="header">
      <h2><%= link_to "<", root_path, class: "back-icon" %></h2>
      <h1>LOGIN</h1>    
      <div class="actions">
        <%= f.submit "Log in" %>
      </div>
    </div>
    <div class="field">
      <%= f.email_field :email, placeholder: "Email Address", autofocus: true, autocomplete: "email" %>
    </div>

    <div class="field">
      <%= f.password_field :password, placeholder: "Password", autocomplete: "current-password" %>
    </div>

  <% end %>
  <%= render "devise/shared/links" %>
</div>

